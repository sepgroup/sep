<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="taskview" prefHeight="494.0" prefWidth="586.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sepgroup.sep.controller.TaskEditorController">
    <children>
        <Label layoutX="208.0" layoutY="15.0" prefHeight="32.0" prefWidth="129.0" text="Edit Task">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Label>
        <Label layoutX="22.0" layoutY="117.0" prefHeight="32.0" prefWidth="74.0" text="Start Date">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Label>
        <Label layoutX="21.0" layoutY="84.0" prefHeight="32.0" prefWidth="129.0" text="Budget">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Label>
        <Label layoutX="22.0" layoutY="52.0" prefHeight="32.0" prefWidth="74.0" text="Name">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Label>
        <Label layoutX="23.0" layoutY="149.0" prefHeight="32.0" prefWidth="63.0" text="Deadline">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Label>
        <Button layoutX="42.0" layoutY="399.0" mnemonicParsing="false" onAction="#onUpdateClicked" prefHeight="60.0" prefWidth="105.0" text="Update" textFill="WHITE">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Button>
        <Button layoutX="213.0" layoutY="399.0" mnemonicParsing="false" onAction="#onDeleteTaskClicked" prefHeight="60.0" prefWidth="105.0" text="Delete" textFill="WHITE">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Button>
        <Button layoutX="364.0" layoutY="399.0" mnemonicParsing="false" onAction="#onCancelClicked" prefHeight="60.0" prefWidth="105.0" text="Cancel" textFill="WHITE">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Button>
        <TextField fx:id="editTaskNameField" layoutX="105.0" layoutY="55.0" />
        <TextField fx:id="editTaskBudgetField" layoutX="105.0" layoutY="88.0" />
        <DatePicker fx:id="editStartDateTaskField" layoutX="104.0" layoutY="120.0" prefHeight="25.0" prefWidth="149.0" />

         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="22.0" layoutY="359.0" prefHeight="32.0" prefWidth="150.0" text="Optimistic time to finish">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="editMostLikelyTime" layoutX="179.0" layoutY="299.0" prefHeight="25.0" prefWidth="29.0" />
      <Label layoutX="213.0" layoutY="296.0" prefHeight="32.0" prefWidth="56.0" text="/ 10 Day">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="editPessimisticTime" layoutX="179.0" layoutY="331.0" prefHeight="25.0" prefWidth="29.0" />
      <TextField fx:id="editOptimisticTime" layoutX="179.0" layoutY="363.0" prefHeight="25.0" prefWidth="29.0" />
      <Label layoutX="213.0" layoutY="327.0" prefHeight="32.0" prefWidth="56.0" text="/ 10 Day">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="213.0" layoutY="360.0" prefHeight="32.0" prefWidth="56.0" text="/ 10 Day">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>

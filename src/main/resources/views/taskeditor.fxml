        <Label layoutX="22.0" layoutY="190.0" prefHeight="32.0" prefWidth="74.0" text="Assignee">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <TextArea fx:id="taskDescriptionArea" layoutX="104.0" layoutY="232.0" prefHeight="51.0" prefWidth="149.0" />
        <Label layoutX="22.0" layoutY="242.0" prefHeight="32.0" prefWidth="74.0" text="Description">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <CheckBox fx:id="completeCheckBox" layoutX="348.0" layoutY="23.0" mnemonicParsing="false" text="Complete?">
            <font>
              <Font name="System Bold" size="12.0" />
          </font>
        </CheckBox>
        <Label fx:id="taskIdLabel" layoutX="285.0" layoutY="23.0" text="[Task id]" />
        <ListView fx:id="dependenciesTaskList" layoutX="383.0" layoutY="96.0" onMouseClicked="#onDependencyTaskListSelected" prefHeight="74.0" prefWidth="156.0" />
        <Label layoutX="409.0" layoutY="60.0" prefHeight="32.0" prefWidth="105.0" text="Dependencies">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <ComboBox fx:id="dependenciesComboBox" layoutX="386.0" layoutY="235.0" onAction="#onDependenciesComboBoxSelected" prefWidth="150.0" />
        <Button fx:id="addSelectedDependencyButton" disable="true" layoutX="400.0" layoutY="270.0" mnemonicParsing="false" onAction="#onAddDependencyClicked" text="Add Dependency" />
        <Button fx:id="deleteSelectedDependencyButton" disable="true" layoutX="399.0" layoutY="179.0" mnemonicParsing="false" onAction="#onDeleteSelectedDependencyClicked" prefHeight="27.0" prefWidth="124.0" text="Delete Selected" />
      <ComboBox fx:id="assigneeComboBox" layoutX="105.0" layoutY="193.0" prefWidth="150.0" />
      <Label layoutX="22.0" layoutY="295.0" prefHeight="32.0" prefWidth="150.0" text="Most Likely time to finish">
